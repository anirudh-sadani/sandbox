var currentDrawn = "mostvisited";

function toggleViews(action) {

eval('$("#'+ currentDrawn + '").toggle();');

eval('$("#'+ action + '").toggle();');
	currentDrawn = action;
    
alert(currentDrawn);

}

function navigateCarousel(slideNum){


    $("#myCarousel").carousel(slideNum);


}

function showECommerce(){
$("#first").hide();

$("#second").show();



}
