INSERT OVERWRITE DIRECTORY '/home/asadani/rampup/hive/spendingtrends' ROW FORMAT DELIMITED FIELDS TERMINATED BY ',' select concat(concat(concat(concat(eventYear, '-'), eventMonth),'-'),eventDate) as eventDateCon, productId, productName, categoryName, price, categoryId, zipcode, userId, userAuthToken, substring(substring(httpURL,1, (instr(httpURL, '?')-1)), instr(httpURL, 'P-')) as trimmed from event_data ED, prod_cat_ref PCR where httpURL like 'data/product%action=%' and substring(substring(httpURL,1, (instr(httpURL, '?')-1)), instr(httpURL, 'P-')) = PCR.productId ;
